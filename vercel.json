{
  "version": 2,
  "builds": [
    {
      "src": "*.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "main.go": {
      "maxDuration": 30
    }
  }
}